{
  "name": "@theatersoft/automation",
  "version": "1.0.0",
  "description": "",
  "main": "dist/automation.js",
  "scripts": {
    "BUS": "BUS=`[ -f .bus ] && cat .bus` AUTH=`[ -f .auth ] && cat .auth`",
    "RUN": "npm run BUS -- node --harmony-async-await",
    "DEBUG": "npm run BUS -- node --harmony-async-await --inspect --debug-brk",
    "build": "time -p node build.js && ls -l dist",
    "build:dist": "DIST=true npm run build",
    "link": "npm link; npm link @theatersoft/automation @theatersoft/bus @theatersoft/device",
    "bus:start": "npm explore @theatersoft/bus -- npm run start",
    "start": "npm run RUN -- dist/start.js",
    "debug": "npm run DEBUG -- dist/start.js",
    "publish": "node build.js publish",
    "watch": "node build.js watch"
  },
  "author": "Rob Huehn <rob@theatersoft.com>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@theatersoft/bus": "~1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@theatersoft/device": "^1.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-babili": "0.0.3",
    "chokidar": "^1.6.1",
    "remote-redux-devtools": "^0.5.7",
    "rollup": "^0.41.4",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-strip": "^1.1.1",
    "shelljs": "^0.7.4"
  }
}
